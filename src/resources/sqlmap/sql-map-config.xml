<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
  PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
  <settings 
     cacheModelsEnabled="true" 
     enhancementEnabled="true"
     lazyLoadingEnabled="true" 
     maxRequests="32" 
     maxSessions="10"
     maxTransactions="5" 
     useStatementNamespaces="true" />

  <!-- 映射存放sql语句的xml文件资源 -->
  <sqlMap resource="resources/sqlmap/ibatis/oracle/TsSupplyDemand.xml"/> <!-- 供求(项目分页示例) -->
  <sqlMap resource="resources/sqlmap/ibatis/oracle/StFileUrl.xml"/><!-- 课件上传 -->
  <sqlMap resource="resources/sqlmap/ibatis/oracle/StUser.xml"/><!-- 用户 -->
</sqlMapConfig>
